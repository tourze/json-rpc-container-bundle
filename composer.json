{
  "name": "tourze/json-rpc-container-bundle",
  "description": "JsonRPC容器模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "tourze/json-rpc-core": "^0.0.6",
    "psr/log": "^3|^2|^1",
    "psr/container": "^1.1|^2.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\JsonRPCContainerBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\JsonRPCContainerBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  }
}