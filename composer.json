{
  "name": "tourze/json-rpc-container-bundle",
  "description": "JsonRPC容器模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "monolog/monolog": "^3.1",
    "psr/container": "^1.1|^2.0",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/service-contracts": "^3.6",
    "symfony/yaml": "^7.3",
    "tourze/json-rpc-core": "0.0.*",
    "tourze/symfony-dependency-service-loader": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-symfony-kernel-test": "0.0.*",
    "tourze/phpunit-symfony-unit-test": "0.0.*",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\JsonRPCContainerBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\JsonRPCContainerBundle\\Tests\\": "tests"
    }
  }
}